{
  "name": "EasyUrl",
  "version": "1.1.1",
  "description": "Simple Url parsing tool",
  "author": "FTVEN",
  "keywords": [
    "url",
    "parsing"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/francetv/easyurl.git"
  },
  "main": "src/EasyUrl.js",
  "scripts": {
    "pre-commit": "npm run -s lint && npm run -s test",
    "postinstall": "bower install",
    "lint": "semistandard $(git diff --name-only --cached --diff-filter=ACMRTUXB | grep '.js$')",
    "test": "grunt test:dev"
  },
  "devDependencies": {
    "bower": "*",
    "captain-git-hook": "https://github.com/francetv/captain-git-hook.git",
    "ftv-mergeup": "*",
    "ftv-prerelease": "*",
    "ftv-release": "*",
    "grunt": "*",
    "grunt-cli": "*",
    "grunt-contrib-clean": "*",
    "grunt-contrib-rename": "*",
    "grunt-contrib-requirejs": "*",
    "grunt-contrib-uglify": "*",
    "grunt-contrib-watch": "*",
    "grunt-mocha-phantomjs": "*",
    "jscoverage": "*",
    "json2htmlcov": "https://github.com/francetv/json2htmlcov.git",
    "mocha-phantomjs": "*",
    "semistandard": "*"
  },
  "directories": {
    "test": "test"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ]
}