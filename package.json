{
  "name": "EasyUrl",
  "version": "2.0.0",
  "description": "Simple Url parsing tool",
  "author": "FTVEN",
  "keywords": [
    "url",
    "parsing"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/francetv/easyurl.git"
  },
